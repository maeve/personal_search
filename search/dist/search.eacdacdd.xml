<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>PS</ShortName>
  <Description>Private Search: Search over your browser history locally</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/x-icon">http://localhost:7172</Image>
  <Url type="text/html" template="http://localhost:7172/index.html">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <moz:SearchForm>http://localhost:7172/index.html</moz:SearchForm>
</OpenSearchDescription>
